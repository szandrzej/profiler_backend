{
  "name": "ApiProfiler",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "node_modules/.bin/mocha test/ --recursive ",
    "run": "nodemon ./bin/www",
    "run-prod": "NODE_ENV=production nodemon ./bin/www",
    "super-run": "forever ./bin/www --minUptime=500 --spinSleepTime=1500",
    "create-user": "node console/createUser",
