    "cors": "^2.7.1",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "express-bearer-token": "^2.1.0",
    "express-validator": "^2.18.0",
    "extend": "^3.0.0",
    "fb": "^0.7.0",
    "jade": "~1.9.2",
    "morgan": "~1.5.3",
    "mysql": "^2.7.0",
    "nodemailer": "^1.4.0",
    "oauth2orize": "^1.0.1",
    "passport": "^0.2.1",
